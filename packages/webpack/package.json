{
  "name": "xc-web-update-notice-webpack",
  "version": "1.0.0",
  "description": "前端版本更新通知 - Webpack 插件",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "webpack",
    "web版本更新通知",
    "version-update"
  ],
  "homepage": "https://github.com/futureyn/xc-web-update-notice",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/futureyn/xc-web-update-notice.git",
    "directory": "packages/webpack"
  },
  "bugs": {
    "url": "https://github.com/futureyn/xc-web-update-notice/issues"
  },
  "author": "nnn",
  "license": "ISC",
  "scripts": {
    "build": "babel index.js --out-dir dist/cjs --env-name cjs --out-file-extension .js && babel index.js --out-dir dist/esm --env-name esm --out-file-extension .js"
  },
  "peerDependencies": {
    "webpack": "^4.0.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "webpack": {
      "optional": false
    }
  }
}
